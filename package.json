{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run build-css\" \"npm run watch-css\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "build-css": "node-sass -rw src/assets/styles/scss -o src/assets/styles/css",
    "watch-css": "npm run build-css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.4.1",
    "@angular/cdk": "^2.0.0-beta.11",
    "@angular/common": "~4.4.3",
    "@angular/compiler": "~4.4.3",
    "@angular/core": "~4.4.3",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "~4.4.3",
    "@angular/http": "~4.4.3",
    "@angular/material": "^2.0.0-beta.11",
    "@angular/platform-browser": "~4.4.3",
    "@angular/platform-browser-dynamic": "~4.4.3",
    "@angular/router": "~4.4.3",
    "@ngx-translate/http-loader": "^2.0.0",
    "angular-in-memory-web-api": "~0.4.6",
    "core-js": "^2.4.1",
    "hammer-timejs": "^1.1.0",
    "npm-sass": "^2.2.1",
    "rxjs": "^5.0.3",
    "systemjs": "0.20.19",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@ngx-translate/core": "^8.0.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "2.6.0",
    "@types/node": "^8.0.28",
    "canonical-path": "0.0.2",
    "concurrently": "^3.5.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.5.3",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "tslint": "^5.7.0",
    "typescript": "^2.2.2"
  },
  "repository": {}
}
